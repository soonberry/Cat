# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Cat' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Cat
  pod ‘R.swift’, ‘~> 4.0.0’

  def testing_pods
    pod 'Quick'
    pod 'Nimble'
  end

  target ‘CatTests' do
    testing_pods
  end

  target ‘CatUITests' do
    testing_pods
  end
end
